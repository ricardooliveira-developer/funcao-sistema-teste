@model WebAtividadeEntrevista.Models.ClienteModel
@{
    ViewBag.Title = "Alterar Cliente";
}
@section scripts{
    <script>
        var clienteId = @Model.Id
        var urlPost = '@Url.Action("Alterar", "Cliente", new { area = "" })';
        var obj = @Html.Raw(Json.Encode(Model));
        var urlRetorno = '@Url.Action("Index", "Cliente", new { area = "" })';
        var urlBeneficiarioList = '@Url.Action("BeneficiarioList", "Beneficiario", new { area = "" })';
        var urlExclusao = '@Url.Action("Excluir", "Beneficiario", new { area = "" })';
    </script>
    @Scripts.Render("~/bundles/altClientes")
    @Scripts.Render("~/bundles/beneficiario")
    @Scripts.Render("~/bundles/listBeneficiarios")
    @Scripts.Render("~/bundles/inputmask")

}
<h2>@Html.Raw(ViewBag.Title)</h2>
@Html.Partial("Forms")
